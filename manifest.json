{
  "manifest_version": 3,
  "name": "Priority Color",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.priority.electrokubi.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
